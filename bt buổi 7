        "#7A4\n",
        "def most_frequent(arr):\n",
        "    count_dict = {}\n",
        "    max_count = 0\n",
        "    result = None\n",
        "\n",
        "    for item in arr:\n",
        "        count_dict[item] = count_dict.get(item, 0) + 1\n",
        "        if count_dict[item] > max_count:\n",
        "            max_count = count_dict[item]\n",
        "            result = item\n",
        "\n",
        "    return result\n"
      ],
