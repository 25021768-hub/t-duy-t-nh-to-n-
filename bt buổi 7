        "nums = [10, 9, 2, 5, 3, 7, 101, 18]\n",
        "n = len(nums)\n",
        "dp = [1] * n\n",
        "\n",
        "for i in range(n):\n",
        "    for j in range(i):\n",
        "        if nums[i] > nums[j]:\n",
        "            dp[i] = max(dp[i], dp[j] + 1)\n",
        "\n",
        "print(max(dp))\n",
