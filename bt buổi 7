        "#W7A1\n",
        "def binary_search(arr, left, right, target):\n",
        "    if left > right:\n",
        "        return -1\n",
        "\n",
        "    mid = (left + right) // 2\n",
        "\n",
        "    if arr[mid] == target:\n",
        "        return mid\n",
        "    elif arr[mid] > target:\n",
        "        return binary_search(arr, left, mid - 1, target)\n",
        "    else:\n",
        "        return binary_search(arr, mid + 1, right, target)\n",
        "\n",
        "arr = [1, 3, 5, 7, 9, 11]\n",
        "target = 7\n",
        "\n",
        "result = binary_search(arr, 0, len(arr) - 1, target)\n",
        "print(result)\n"
      ]
    },
    {
      "cell_type": "code",
