        "#7A5\n",
        "def count_pairs(arr, X):\n",
        "    count = 0\n",
        "    n = len(arr)\n",
        "    for i in range(n):\n",
        "        for j in range(i + 1, n):\n",
        "            if arr[i] + arr[j] == X:\n",
        "                count += 1\n",
        "    return count\n"
