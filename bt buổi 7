        "#W7A2\n",
        "def count_occurrences(arr, x) -> int:\n",
        "    count = 0\n",
        "    for item in arr:\n",
        "        if item == x:\n",
        "            count += 1\n",
        "    return count\n",
        "arr = [1, 2, 3, 2, 4, 2, 5]\n",
        "x = 2\n",
        "\n",
        "print(count_occurrences(arr, x))\n"
      ],
