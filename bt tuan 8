        "#W8A4\n",
        "class Calculator():\n",
        "  def __init__(self,a,b):\n",
        "    self.a = a\n",
        "    self.b = b\n",
        "  def set_numbers(self,a,b):\n",
        "    self.a = a\n",
        "    self.b = b\n",
        "  def add(self):\n",
        "    return self.a + self.b\n",
        "  def subtract(self):\n",
        "    return self.a - self.b\n",
        "  def product(self):\n",
        "    return self.a * self.b\n",
        "  def divide(self):\n",
        "    if self.b == 0:\n",
        "      return \"Không thể chia cho 0\"\n",
        "    return self.a / self.b\n",
        "  def power(self):\n",
        "    return self.a ** self.b\n",
        "\n",
        "  def mod(self):\n",
        "    if self.b == 0:\n",
        "      return \"Không thể chia cho 0\"\n",
        "    return self.a % self.b\n",
        "\n",
        "a, b = map(float, input(\"Nhập hai số ban đầu: \").split())\n",
        "calc = Calculator(a, b)\n",
        "\n",
        "while True :\n",
        "  print(\"\\n MENU \")\n",
        "  print(\"1. Cộng\")\n",
        "  print(\"2. Trừ\")\n",
        "  print(\"3. Nhân\")\n",
        "  print(\"4. Chia\")\n",
        "  print(\"5. Lũy thừa\")\n",
        "  print(\"6. Chia lấy dư\")\n",
        "  print(\"7. Cập nhật 2 số mới\")\n",
        "  print(\"8. Thoát\")\n",
        "  choice = input(\"Nhập lựa chọn của bạn: \")\n",
        "\n",
        "  if choice == \"1\":\n",
        "    print(\"Kết quả phép cộng:\", calc.add())\n",
        "  elif choice == \"2\":\n",
        "    print(\"Kết quả phép trừ:\", calc.subtract())\n",
        "  elif choice == \"3\":\n",
        "    print(\"Kết quả phép nhân:\", calc.product())\n",
        "  elif choice == \"4\":\n",
        "    print(\"Kết quả phép chia:\", calc.divide())\n",
        "  elif choice == \"5\":\n",
        "    print(\"Kết quả phép lũy thừa:\", calc.power())\n",
        "  elif choice == \"6\":\n",
        "    print(\"Kết quả phép chia lấy dư:\", calc.mod())\n",
        "  elif choice == \"7\":\n",
        "    a, b = map(float, input(\"Nhập hai số mới: \").split())\n",
        "    calc.set_numbers(a, b)\n",
        "    print(\"Đã cập nhật số mới!\")\n",
        "  elif choice == \"8\":\n",
        "    print(\"Chương trình kết thúc!\")\n",
        "    break\n",
        "\n"
      ],
