        "#W8A3\n",
        "import math\n",
        "class Point():\n",
        "  def __init__(self,x,y):\n",
        "    self.x = x\n",
        "    self.y = y\n",
        "  def is_origin(self):\n",
        "    return self.x == 0 and self.y == 0\n",
        "  def is_on_x_axis(self):\n",
        "    return self.x == 0 and self.y != 0\n",
        "  def is_on_y_axis(self):\n",
        "    return self.y == 0 and self.x != 0\n",
        "  def distance(self,other):\n",
        "    return math.sqrt((self.x - other.x)**2 + (self.y - other.y)**2)\n",
        "\n",
        "\n",
        "x,y = map(float,input().split())\n",
        "M = Point(x,y)\n",
        "O = Point(0,0)\n",
        "if M.is_origin():\n",
        "    print(\"M là gốc tọa độ\")\n",
        "elif M.is_on_y_axis():\n",
        "    print(\"M nằm trên trục tung\")\n",
        "elif M.is_on_x_axis():\n",
        "    print(\"M nằm trên trục hoành\")\n",
        "else:\n",
        "    print(\"M không nằm trên trục nào\")\n",
        "\n",
        "distance = M.distance(O)\n",
        "print(f\"Khoảng cách từ M đến gốc tọa độ là {distance:.2f}\")"
      ],
