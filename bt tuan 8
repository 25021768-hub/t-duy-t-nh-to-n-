        "#W8A5\n",
        "class ShoppingCart:\n",
        "  def __init__(self):\n",
        "    self.product = []\n",
        "  def add_product(self, name, price):\n",
        "    self.product.append({\"name\" : name, \"price\" : price})\n",
        "  def remove_product(self, name):\n",
        "    for product in self.product:\n",
        "      if product[\"name\"] == name:\n",
        "        self.product.remove(product)\n",
        "        return True\n",
        "    return False\n",
        "  def is_empty(self):\n",
        "    return len(self.product) == 0\n",
        "  def total_price(self):\n",
        "    total = 0\n",
        "    for product in self.product:\n",
        "      total += product[\"price\"]\n",
        "    return total\n",
        "  def display(self):\n",
        "    if self.is_empty():\n",
        "      print(\"Giỏ hàng đang trống\")\n",
        "    else :\n",
        "      print(\"Danh sách sản phẩm trong giỏ hàng:\")\n",
        "      for product in self.product:\n",
        "        print(f\"- {product['name']}: {product['price']}\")\n",
        "  def clear_cart(self):\n",
        "    self.product.clear()\n",
        "\n",
        "cart = ShoppingCart()\n",
        "\n",
        "while True:\n",
        "  print(\"\\n----- MENU -----\")\n",
        "  print(\"2. Thêm sản phẩm\")\n",
        "  print(\"3. Xóa sản phẩm theo tên\")\n",
        "  print(\"4. Kiểm tra giỏ hàng có rỗng không\")\n",
        "  print(\"5. Tính tổng tiền\")\n",
        "  print(\"6. Hiển thị giỏ hàng\")\n",
        "  print(\"7. Xóa toàn bộ giỏ hàng\")\n",
        "\n",
        "  choice = input(\"Nhập lựa chọn (2-7): \")\n",
        "\n",
        "  if choice == \"2\":\n",
        "    name = input(\"Nhập tên sản phẩm: \")\n",
        "    price = float(input(\"Nhập giá sản phẩm: \"))\n",
        "    cart.add_product(name, price)\n",
        "    print(\"Đã thêm sản phẩm!\")\n",
        "\n",
        "  elif choice == \"3\":\n",
        "    name = input(\"Nhập tên sản phẩm cần xóa: \")\n",
        "    if cart.remove_product(name):\n",
        "      print(\"Đã xóa sản phẩm!\")\n",
        "    else:\n",
        "      print(\"Không tìm thấy sản phẩm!\")\n",
        "\n",
        "  elif choice == \"4\":\n",
        "    if cart.is_empty():\n",
        "      print(\"Giỏ hàng rỗng\")\n",
        "    else:\n",
        "      print(\"Giỏ hàng không rỗng\")\n",
        "\n",
        "  elif choice == \"5\":\n",
        "    print(f\"Tổng tiền: {cart.total_price():.2f}\")\n",
        "\n",
        "  elif choice == \"6\":\n",
        "    cart.display()\n",
        "\n",
        "  elif choice == \"7\":\n",
        "    cart.clear_cart()\n",
        "    print(\"Đã xóa toàn bộ giỏ hàng!\")\n",
        "\n",
        "  else:\n",
        "    print(\"Lựa chọn không hợp lệ!\")\n",
        "\n",
        "  exit_choice = input(\"Có muốn thoát khỏi chương trình không? \")\n",
        "  if exit_choice.lower() == \"yes\":\n",
        "    print(\"Chương trình kết thúc!\")\n",
        "    break"
      ],
