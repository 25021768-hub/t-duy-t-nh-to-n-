        "#W8A9\n",
        "class Student():\n",
        "  def __init__(self,name):\n",
        "    self.name = name\n",
        "    self.scores = {}\n",
        "  def add_score(self,subject,score):\n",
        "    self.scores[subject] = score\n",
        "  def avg(self):\n",
        "    if not self.scores:\n",
        "      return 0\n",
        "    return sum(self.scores.values()) / len(self.scores)\n",
        "  def rank(self):\n",
        "    a = self.avg()\n",
        "    if a >= 8:\n",
        "      return \"Excellent\"\n",
        "    elif a >= 6.5:\n",
        "      return \"Good\"\n",
        "    elif a >= 5:\n",
        "      return \"Average\"\n",
        "    else:\n",
        "      return \"Poor\"\n",
        "\n",
        "name = input().strip()\n",
        "n = int(input())\n",
        "student = Student(name)\n",
        "for _ in range(n):\n",
        "  subject, score = input().split()\n",
        "  student.add_score(subject, float(score))\n",
        "\n",
        "avg_score = student.avg()\n",
        "rank = student.rank()\n",
        "\n",
        "print(f\"{student.name} {avg_score:.2f} {rank}\")\n"
      ],
