        "#W8A1\n",
        "class Cylinder:\n",
        "    def __init__(self, radius, height):\n",
        "        self.radius = radius\n",
        "        self.height = height\n",
        "        self.pi = 3.14\n",
        "\n",
        "    def get_surface_area(self):\n",
        "        return 2 * self.pi * self.radius * (self.radius + self.height)\n",
        "\n",
        "    def get_volume(self):\n",
        "        return self.pi * (self.radius ** 2) * self.height\n",
        "\n",
        "data = input().split()\n",
        "r = float(data[0])\n",
        "h = float(data[1])\n",
        "cylinder = Cylinder(r, h)\n",
        "surface_area = cylinder.get_surface_area()\n",
        "volume = cylinder.get_volume()\n",
        "print(f\"{surface_area:.2f} {volume:.2f}\")"
      ]
    },
