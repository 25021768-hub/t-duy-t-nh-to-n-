      "source": [
        "#W8A8\n",
        "class BankAccount:\n",
        "  def __init__(self, owner, balance):\n",
        "    self.owner = owner\n",
        "    self.balance = balance\n",
        "  def deposit(self,x):\n",
        "    self.balance += x\n",
        "  def withdraw(self,x):\n",
        "    if self.balance >= x:\n",
        "      self.balance -= x\n",
        "  def __str__(self):\n",
        "    return f\"{self.owner} : {self.balance:.2f}\"\n",
        "\n",
        "owner, initial_balance = input().split()\n",
        "balance = float(initial_balance)\n",
        "\n",
        "account = BankAccount(owner, balance)\n",
        "\n",
        "n = int(input())\n",
        "for _ in range(n):\n",
        "  cmd, x = input().split()\n",
        "  x = float(x)\n",
