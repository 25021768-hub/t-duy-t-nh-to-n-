      "cell_type": "code",
      "source": [
        "#W8A10\n",
        "class Book:\n",
        "  def __init__(self, title, author, year):\n",
        "    self.title = title\n",
        "    self.author = author\n",
        "    self.year = year\n",
        "\n",
        "class Library:\n",
        "  def __init__(self):\n",
        "    self.books = []\n",
        "\n",
        "  def add(self,book):\n",
        "    self.books.append(book)\n",
        "\n",
        "  def find_by_year(self, y):\n",
        "    count = 0\n",
        "    for book in self.books:\n",
        "      if book.year == y:\n",
        "        count += 1\n",
        "    return count\n",
        "\n",
        "  def count_by_author(self,name):\n",
        "    count = 0\n",
        "    for book in self.books:\n",
        "      if book.author == name:\n",
        "        count += 1\n",
        "    return count\n",
        "\n",
        "\n",
        "n = int(input())\n",
        "library = Library()\n",
        "for _ in range(n):\n",
        "    line = input().strip()\n",
        "\n",
        "    if line.startswith(\"ADD\"):\n",
        "        _, data = line.split(\" \", 1)\n",
        "        title, author, year = data.split(\";\")\n",
        "        book = Book(title, author, int(year))\n",
        "        library.add(book)\n",
        "\n",
        "    elif line.startswith(\"COUNTYEAR\"):\n",
        "        _, year = line.split()\n",
        "        print(library.find_by_year(int(year)))\n",
        "\n",
        "    elif line.startswith(\"COUNT\"):\n",
        "        _, author = line.split(\" \", 1)\n",
        "        print(library.count_by_author(author))\n",
        "\n"
      ],
      "metadata": {
