        "#W8A6\n",
        "class Rectangle:\n",
        "  def __init__(self,w,h):\n",
        "    self.w = w\n",
        "    self.h = h\n",
        "  def area(self):\n",
        "    return self.w * self.h\n",
        "  def perimeter(self):\n",
        "    return 2 * (self.w + self.h)\n",
        "  def scale(self,k):\n",
        "    self.w *= k\n",
        "    self.h *= k\n",
        "\n",
        "w,h,k = map(float,input().split())\n",
        "rect = Rectangle(w,h)\n",
        "rect.scale(k)\n",
        "print(f\"{rect.area():.2f}, {rect.perimeter():.2f}\")\n",
        "\n"
      ],
